{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "amd": true,
        "jquery": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": "eslint-config-hfe",
    "globals": {
        "Vue": true,
        "superagent": true,
        "App": true,
        "wx": true,
        "getApp": true,
        "Page": true
    },
    "rules": {
        "strict": 0,
        "no-new": 0,
        "no-console": "off",
        "no-invalid-this": "off"
    }
}
